+
+static ssize_t store_vdemand_vsel(struct cpufreq_policy *policy, char *buf,
+                                                size_t count) {
+        u8 volt[3], i;
+
+        mutex_lock(&vdemand_mutex);
+
+        if(sscanf(buf, "%hhu %hhu %hhu", &volt[2],&volt[1],&volt[0])) {
+                for(i=0;i < 3;i++) {
+                        if((volt[i] < VDD1_VSEL_MIN) ||
+				(volt[i] > VDD1_VSEL_MAX)) {
+                                printk("CPUfreq: invalid vsel! Resetting to default!\n");
+				volt[i]=vdd1_vsel_def[i];
+                                break;
+                        }
+                }
+                if(i == 3) memcpy(v_volt,volt,sizeof(volt));
+        }
+        else printk("CPUfreq: missing vsel values\n");
+
+        mutex_unlock(&vdemand_mutex);
+        return count;
+}
+
+static ssize_t show_vdemand_poll(struct cpufreq_policy *policy, char *buf) {
+        return sprintf(buf, "%hu\n", vdemand_poll_rate);
+}
+
+static ssize_t store_vdemand_poll(struct cpufreq_policy *policy, char *buf,
+                                                size_t count) {
+        unsigned int i;
+
+        if(sscanf(buf, "%u", &i)) {
+                if((i < VDEMAND_POLL_RATE_MIN) && (i > VDEMAND_POLL_RATE_MAX))
+                        printk("CPUfreq: invalid \n");
+                else {
+                        vdemand_poll_rate=i;
+                }
+        }
+        else printk("CPUfreq: missing value\n");
+
+        return count;
+}
