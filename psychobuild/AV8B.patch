+						vdd2_vsel_def[0]);
+}
+CPUFREQ_OVERRIDE_ATTR2(vdemand_vsel_vdd1_default,0444,
+			show_vdemand_vsel_vdd1_def);
+CPUFREQ_OVERRIDE_ATTR2(vdemand_vsel_vdd2_default,0444,
+			show_vdemand_vsel_vdd2_def);
